<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightGray"
    tools:context=".GalleryFragment">

    <ImageView
        android:id="@+id/vector4"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fh1"
        app:layout_constraintStart_toEndOf="@+id/fh2"
        app:layout_constraintTop_toBottomOf="@+id/vector2"
        app:layout_constraintVertical_bias="0.692" />

    <TextView
        android:id="@+id/textView"
        style="@style/newStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/galleryTitle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.536" />

    <ImageView
        android:id="@+id/fh1"
        android:layout_width="270dp"
        android:layout_height="149dp"
        android:layout_marginBottom="12dp"
        android:src="@drawable/fhteamfour"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <ImageView
        android:id="@+id/fh2"
        android:layout_width="233dp"
        android:layout_height="151dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/fhteamtwo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/fh3"
        android:layout_width="209dp"
        android:layout_height="157dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/fhteamthree"
        app:layout_constraintBottom_toTopOf="@+id/fh1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toEndOf="@+id/textView" />

    <ImageView
        android:id="@+id/vector2"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:layout_marginEnd="46dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toStartOf="@+id/fh3"
        app:layout_constraintHorizontal_bias="0.608"
        app:layout_constraintStart_toEndOf="@+id/fh4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.791" />

    <ImageView
        android:id="@+id/fh4"
        android:layout_width="274dp"
        android:layout_height="147dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/fhteamone"
        app:layout_constraintEnd_toStartOf="@+id/textView"
        app:layout_constraintHorizontal_bias="0.277"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>