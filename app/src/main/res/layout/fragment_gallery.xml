<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightGray"
    tools:context=".GalleryFragment">

    <ImageView
        android:id="@+id/vector2"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toTopOf="@+id/fh2"
        app:layout_constraintEnd_toStartOf="@+id/fh3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fh4" />

    <ImageView
        android:id="@+id/vector3"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toTopOf="@+id/fh1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fh2"
        app:layout_constraintTop_toBottomOf="@+id/fh3" />

    <ImageView
        android:id="@+id/vector4"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fh1"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fh2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        style="@style/newStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/galleryTitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/fh1"
        android:layout_width="275dp"
        android:layout_height="159dp"
        android:src="@drawable/fhteamfour"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.97"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fh3"
        app:layout_constraintVertical_bias="0.9" />

    <ImageView
        android:id="@+id/fh2"
        android:layout_width="259dp"
        android:layout_height="170dp"
        android:layout_marginEnd="148dp"
        android:src="@drawable/fhteamtwo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fh4"
        app:layout_constraintVertical_bias="0.483" />

    <ImageView
        android:id="@+id/fh3"
        android:layout_width="215dp"
        android:layout_height="184dp"
        android:layout_marginTop="208dp"
        android:src="@drawable/fhteamthree"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/fh4"
        android:layout_width="275dp"
        android:layout_height="156dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="148dp"
        android:src="@drawable/fhteamone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/vector1"
        android:layout_width="82dp"
        android:layout_height="73dp"
        android:src="@drawable/ic_baseline_sports_hockey_24"
        app:layout_constraintBottom_toTopOf="@+id/fh3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.338"
        app:layout_constraintStart_toEndOf="@+id/fh4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.711" />

</androidx.constraintlayout.widget.ConstraintLayout>